{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "betterwx-ui",
  "version": "1.0.1",
  "identifier": "com.betterwx-ui.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:51420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "betterwx-ui",
        "width": 800,
        "minWidth": 800,
        "height": 400,
        "minHeight": 400
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  }
}